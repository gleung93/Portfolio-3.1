<section>
  <div class="container">
    <div class="row">
      <div class="col-md-6 text-center {{text-color}}">
        <h3>{{ sub-heading }}</h3>
        <p>{{ body }} </p>
      </div>
    </div>
    {{#each item}}
    <div class="row">
      <div class="col-xs-12">
        <img class="img-block" src="{{img-src}}">
      </div>
    </div>
    {{/each}}
  </div>
</section